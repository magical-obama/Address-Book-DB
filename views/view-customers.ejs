<!DOCTYPE html>
<html lang="en">
<head>
    <%= await include('./partials/head.ejs') %>
    <link rel="stylesheet" href="/css/view-customers.css">
    <title>View Contacts</title>
</head>
<body>
    <% customers.forEach((item, index) => { %>
        <div class="customer">
            <h2><%= item.name %></h2>
            <p><%= item.username %></p>
            <p><%= item.address %></p>
            <p><%= item.accounts %></p>
            <p><%= item.email %></p>
            <p><%= item.birthdate %></p>
        </div>
    <% }) %>
</body>
</html>